{
  "name": "velvet",
  "version": "1.1.0",
  "description": "Velvet is a modular, fast, static-site generator that looks a lot like Jekyll and can be easily incorporated into Gulp-based workflows.",
  "engines": {
    "node": ">= 5.0.0"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronik-design/velvet.git"
  },
  "keywords": [
    "gulp",
    "velvet",
    "static-site",
    "generator",
    "ssg"
  ],
  "author": "Michael Shick <m@shick.us>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ronik-design/velvet/issues"
  },
  "homepage": "https://github.com/ronik-design/velvet#readme",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.9.0",
    "eslint-config-ronik": "^1.0.3"
  },
  "dependencies": {
    "hoek": "^4.0.0",
    "moment-timezone": "^0.5.1",
    "nunjucks": "^2.3.0",
    "velvet-core": "^1.1.1",
    "velvet-nunjucks": "^1.0.2"
  },
  "optionalDependencies": {
    "markdown-it": "^6.0.1"
  },
  "eslintConfig": {
    "extends": "ronik",
    "rules": {
      "camelcase": 0
    }
  }
}
